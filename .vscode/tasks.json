{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build enum_int example",
			"type": "shell",
			"command": "cl.exe /EHsc /std:c++latest /I.. /Fe:enum_int_example.exe enum_int.cpp enum_int_main.cpp",
			"options": {
				"cwd": "${workspaceFolder}/examples"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "build enum_string example",
			"type": "shell",
			"command": "cl.exe /EHsc /std:c++latest /I.. /Fe:enum_string_example.exe enum_string.cpp enum_string_main.cpp",
			"options": {
				"cwd": "${workspaceFolder}/examples"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build"
		}
	]
}